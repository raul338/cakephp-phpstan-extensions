{
    "name": "raul338/cakephp-phpstan-extensions",
    "description": "Services to aid phpstan analysis on CakePHP projects",
    "type": "phpstan-extension",
    "keywords": [
        "cakephp",
        "stub",
        "test",
        "static",
        "analysis"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "RaÃºl Arellano",
            "email": "raul338@elhacker.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "phpstan/phpstan": "^0.11"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^4.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
        "cakephp/cakephp": "^3.8",
        "friendsofcake/crud": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Raul338\\Phpstan\\Cake\\": "src"
        }
    },
    "scripts": {
        "cs-check": "vendor/bin/phpcs --colors -p -s ./src",
        "cs-fix": "vendor/bin/phpcbf --colors -p -s ./src",
        "phpstan": "phpstan analyze -c phpstan.neon --ansi --memory-limit=-1"
    }
}
